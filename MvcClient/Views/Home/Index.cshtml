@using Microsoft.AspNetCore.Authentication

@{
    ViewData["Title"] = "Home Page";
}

<h2>Claims</h2>

<dl>
    @foreach (var item in User.Claims)
    {
        <dt>@item.Type</dt>
        <dd>@item.Value</dd>
    }
</dl>


<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>
